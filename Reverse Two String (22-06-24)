

public class Main
{
	public static void main(String[] args) {
		String s="Ashu";
		String s1="janvi";
		String res="";
		for(int i=0,j=s1.length()-1;i<s.length()||j>=0;i++,j--){
		    if(i<s.length()){
		        res=res+s.charAt(i);
		        i++;
		    }
		    if(j>=0){
		        res=res+s1.charAt(j);
		        j--;
		    }
		}
		System.out.println(res);
	}
}
