import java.util.*;
public class Main
{
	public static void main(String[] args) {
	    List<Integer>l1=new ArrayList<>();
	    int a[]={2,4,5,6,9,8};
	    int b[]={0,9,8,3,1,6};
	    for(int i=0;i<a.length;i++){
	        for(int j=0;j<b.length;j++){
	            if(a[i]==b[j]){
	                l1.add(a[i]);
	               // System.out.print(a[i]);
	            }
	        }
	    }
	    
		System.out.println(l1);
	}
}
