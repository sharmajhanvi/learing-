import java.util.*;
public class Main
{
	public static void main(String[] args) {
	    int arr1[]={3,9,8,6,4,7,2,99};
	    int arr2[]={5,8,99,89,76,45,9};
	    List<Integer>l1=new ArrayList<>();
	    for(int i=0;i<arr1.length;i++){
	        for(int j=0;j<arr2.length;j++){
	            if(arr1[i]==arr2[j]){
	                l1.add(arr1[i]);
	            }
	            
	        }
	    }
	    System.out.println(l1);
	}
}
