public class Main
{
  
	public static void main(String[] args) {
	    String s1="janvi";
	    String s2="ashu";
	    String res="";
	    for(int i=0,j=s2.length()-1;i<s1.length()||j>=0;i++,j--){
	        if(i<s1.length()){
	            res=res+s1.charAt(i);
	             }
	             if(j>=0){
	                 res=res+s2.charAt(j);
	             }
	    }
	    System.out.println(res);
	  
	}
}
