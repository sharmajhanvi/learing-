import java.util.*;
public class Main
{
	public static void main(String[] args) {
	    Set<Integer>s1=new HashSet<>();
	    int arr1[]={2,5,7,4,3,1,3};//5,7,1,3
	    int arr2[]={3,6,7,3,9,5,1};
	    int sum=0;
	    for(int i=0;i<arr1.length;i++){
	        for(int j=0;j<arr2.length;j++){
	            if(arr1[i]==arr2[j]){
	                sum=sum+arr1[i];
	              
	            }
	        }
	    }
	    System.out.println(sum);
	    
	}
}
