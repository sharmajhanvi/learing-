import java.util.*;
public class Main
{
    public static void main(String[] args) {
      String s="shashikant";
      Map<Character,Integer> m1=new HashMap<>();
      for(int i=0;i<s.length();i++){
          char c=s.charAt(i);
         m1.put(c,m1.getOrDefault(c,0) + 1 );
      }
      for(Map.Entry<Character,Integer>it:m1.entrySet()){
          char c1=it.getKey();
          int c2=it.getValue();
          System.out.println(c1+" "+c2);
      }
	}
}
