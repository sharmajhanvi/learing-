import java.util.*;
public class Main
{
	public static void main(String[] args) {
	    String s1="nana";
	    String s2="anan";
	    s1=s1.toLowerCase();
	    s2=s2.toLowerCase();
	    char []c1=s1.toCharArray();
	    char []c2=s2.toCharArray();
	    Arrays.sort(c1);
	    Arrays.sort(c2);
	    boolean res=Arrays.equals(c1,c2);
	    if(res==true){
	        System.out.println("Anagram");
	    }
	    else{
	        System.out.println("Not an Anamgram");
	    }
	}
}
